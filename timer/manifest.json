{
  "name": "Agar.io Merge Timer",

  "short_name": "AgarTimer",

  "description": "A dynamic timer to inform your tactics in the crucial moments between split and merge.",

  "version": "1.0.0",

  "author": "Evan Sandhoefner",
  
  "content_scripts": [
    {
      "matches": ["http://agar.io/*"],
      "js": ["d3.js","page.js","jquery.js"]
    }
  ],

  "options_page": "options.html",
  
  "browser_action": {
    "default_title": "Time to current page",
    "default_icon": "doge-16-.png",
    "default_popup": "popup.html"
  },
  
  "icons": {
    "16": "doge-16-.png",
    "48": "doge-48-.png",
    "128": "doge-128-.png"
  },
  
  "manifest_version": 2
}